{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"plugin:react/jsx-runtime",
		"plugin:testing-library/react",
		"standard",
		"eslint-config-prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": "warn",
		"react/prop-types": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
